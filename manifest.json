{
  "manifest_version": 2,
  "name": "naisReader",
  "short_name": "naisReader",
  "description": "Makes the ones nais 2 read",
  "version": "0.0.1",
  "minimum_chrome_version": "38",

  "icons": {
    "16": "assets/icon_16.png",
    "128": "assets/icon_128.png"
  },

  "background": {
    "scripts": ["background.js"]
  },
    
  "permissions": [
    "tabs",
    "activeTab"
  ],
  
  "browser_action": {
    "default_title": "Make the ones nais 2 read"
  }
}

